[global]
ioengine=psync
size=256M
direct=0
iodepth=1
norandommap
group_reporting=1
end_fsync=1

directory=/mnt/test/
filename_format=fio_$jobnum

#time_based=1
#runtime=60
#ramp_time=10

[read]
rw=read
invalidate=0

[write]
rw=write

[randread]
rw=randread
invalidate=0

[randwrite]
rw=randwrite

[randrw]
rw=randrw
rwmixread=50
